<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zstu.edu.mapper.RoomsMapper">

    <select id="selectByFloorsId" resultType="com.zstu.edu.entity.Rooms">
        select * from rooms where floors_id=#{floorsId}
    </select>
    <select id="selectByBuildingsId" resultType="com.zstu.edu.entity.Rooms">
        select * from rooms where building_id=#{buildingsId}
    </select>
</mapper>
